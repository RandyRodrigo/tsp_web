---
import Layout from '@/layouts/Layout.astro';
import Bento from '@/components/Bento.astro';
import Intro from '@/components/Intro.astro';
import Clients from '@/components/Clients.astro';
import Video from '@/components/Video.astro';
import { getI18N } from '@/i18n';
import Numeros from '@/components/Numeros.astro';
import Galeria from '../Galeria.astro';

const { currentLocale } = Astro
const i18n = getI18N({ currentLocale })
---

<Layout
    title={i18n.SEO_TITLE},
    description={i18n.SEO_DESCRIPTION}
>
	<Video />

	<main>
		<Intro />
		<Numeros />
		<Clients />
		<Bento />
		<Galeria />
	</main>
</Layout>

